{
  "name": "chatalog-backend",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -k -n server,typecheck -c auto \"tsx watch src/server.ts\" \"tsc --noEmit --watch\"",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20"
  }
}
